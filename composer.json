{
    "name": "lgrevelink/laravel-simple-jwt",
    "description": "Utilties for using the php-simple-jwt package in Laravel/Lumen projects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Grevelink"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/console": "^5.7|^6.0",
        "illuminate/support": "^5.7|^6.0",
        "illuminate/validation": "^5.7|^6.0",
        "lgrevelink/php-simple-jwt": "~0.2"
    },
    "require-dev": {
        "laravel/framework": "^5.7|^6.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LGrevelink\\LaravelSimpleJWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LGrevelink\\LaravelSimpleJWT\\Providers\\SimpleJwtServiceProvider"
            ]
        }
    }
}
